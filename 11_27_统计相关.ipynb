   ],
   "source": [
    "import numpy as np\n",
    "\n",
    "np.random.seed(100)\n",
    "a = np.random.randint(1, 10, [5, 3])\n",
    "\n",
    "b = np.amax(a, axis=1)\n",
    "print(a)\n",
    "print(b)"
